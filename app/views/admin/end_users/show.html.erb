 <h2><%= @end_user.name%>さんの会員情報</h2>

 <table>
 <tr>
    <td>顧客ID</td>
    <td><%= @end_user.id%><span><%= @end_user.is_deleted%>会員</span></td>
 </tr>
 <tr>
    <td>氏名</td>
    <td><%= @end_user.name%></td>
 </tr>
 <tr>
    <td>フリガナ</td>
    <td><%= @end_user.name_kana%></td>
 </tr>
 <tr>
    <td>郵便番号</td>
    <td><%= @end_user.postal_code%></td>
 </tr>
 <tr>
    <td>住所</td>
    <td><%= @end_user.address%></td>
 </tr>
 <tr>
    <td>電話番号</td>
    <td><%= @end_user.telephone_number%></td>
 </tr>
 <tr>
    <td>メールアドレス</td>
    <td><%= @end_user.email%></td>
 </tr>
 <tr>
    <td><%= link_to "編集する",edit_admin_end_user_path(@end_user),class: "btn btn primary"%></td>
    <td></td>
 </tr>
 </table>