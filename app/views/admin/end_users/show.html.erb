 <h2><%= @end_user.name%>さんの会員情報</h2>
<div class="col-md-6">
 <table class="table table-borderless">
 <tr>
    <td>顧客ID</td>
    <td class="ml-4"><%= @end_user.id%><span class="is_deleted ml-2"><%= @end_user.is_deleted%>会員</span></td>
 </tr>
 <tr>
    <td>氏名</td>
    <td class="ml-4"><%= @end_user.name%></td>
 </tr>
 <tr>
    <td>フリガナ</td>
    <td class="ml-4"><%= @end_user.name_kana%></td>
 </tr>
 <tr>
    <td>郵便番号</td>
    <td class="ml-4"><%= @end_user.postal_code%></td>
 </tr>
 <tr>
    <td>住所</td>
    <td class="ml-4"><%= @end_user.address%></td>
 </tr>
 <tr>
    <td>電話番号</td>
    <td class="ml-4"><%= @end_user.telephone_number%></td>
 </tr>
 <tr>
    <td>メールアドレス</td>
    <td class="ml-4"><%= @end_user.email%></td>
 </tr>
 <tr>
    <td><%= link_to "編集する",edit_admin_end_user_path(@end_user),class: "btn btn-primary"%></td>
    <td><%= link_to "注文履歴一覧を見る",admin_orders_path,class: "btn btn-primary"%></td>
 </tr>
 </table>
 </div>